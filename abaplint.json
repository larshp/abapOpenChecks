{
  "global": {
    "files": "/src/**/*.*",
    "skipGeneratedGatewayClasses": true,
    "skipGeneratedPersistentClasses": true,
    "skipGeneratedFunctionGroups": true
  },
  "dependencies": [
    {
      "url": "https://github.com/abaplint/deps",
      "folder": "/deps",
      "files": "/src/**/*.*"
    }
  ],
  "syntax": {
    "version": "v740sp02",
    "errorNamespace": "^(Z|Y)",
    "globalConstants": [
      "sana_tok_field_def",
      "sana_tok_word",
      "seoc_category_exception",
      "seoc_category_p_agent",
      "seoc_category_test_class",
      "seoc_category_webdynpro_class",
      "seoc_version_inactive",
      "seor_reltype_inheritance",
      "slin_errlv",
      "slin_todo"
    ],
    "globalMacros": ["_code", "_test", "zzaoc_top", "zzaoc_fill_att", "zzaoc_fill_att_rb", "zzaoc_popup"]
  },
  "rules": {
    "identical_contents": true,
    "many_parenthesis": true,
    "prefer_xsdbool": true,
    "begin_single_include": true,
    "check_subrc": false,
    "cyclomatic_complexity": {
      "max": 30
    },
    "downport": false,
    "forbidden_pseudo_and_pragma": false,
    "forbidden_void_type": false,
    "identical_conditions": false,
    "line_break_multiple_parameters": false,
    "line_break_style": false,
    "names_no_dash": true,
    "parser_missing_space": true,
    "prefer_inline": false,
    "prefer_is_not": false,
    "reduce_string_templates": true,
    "unused_types": false,
    "use_bool_expression": false,
    "use_line_exists": false,
    "forbidden_identifier": false,
    "try_without_catch": true,
    "unknown_types": false,
    "prefix_is_current_class": false,
    "unused_variables": false,
    "allowed_object_naming": true,
    "check_comments": false,
    "fully_type_constants": true,
    "keep_single_parameter_on_one_line": false,
    "prefer_returning_to_exporting": true,
    "selection_screen_naming": false,
    "sicf_consistency": true,
    "sql_escape_host_variables": false,
    "xml_consistency": true,
    "check_abstract": true,
    "check_no_handler_pragma": true,
    "newline_between_methods": false,
    "types_naming": false,
    "chain_mainly_declarations": true,
    "check_text_elements": true,
    "abapdoc": false,
    "allowed_object_types": false,
    "ambiguous_statement": true,
    "begin_end_names": true,
    "check_ddic": true,
    "check_include": true,
    "check_transformation_exists": true,
    "empty_structure": true,
    "form_tables_obsolete": true,
    "implement_methods": true,
    "in_statement_indentation": true,
    "local_testclass_location": true,
    "main_file_contents": true,
    "release_idoc": true,
    "rfc_error_handling": false,
    "space_before_dot": true,
    "type_form_parameters": true,
    "use_new": false,
    "7bit_ascii": true,
    "avoid_use": {
      "define": false,
      "endselect": true,
      "defaultKey": false,
      "execSQL": true,
      "kernelCall": true,
      "describeLines": true,
      "communication": true,
      "statics": false,
      "break": true,
      "systemCall": true
    },
    "check_syntax": false,
    "class_attribute_names": {
      "exclude": ["zcl_aoc_scan\\.clas\\.abap"],
      "ignoreExceptions": true,
      "ignoreLocal": false,
      "statics": "^G._.*$",
      "constants": ".",
      "ignoreInterfaces": false,
      "instance": "^M._.*$"
    },
    "cloud_types": true,
    "colon_missing_space": true,
    "commented_code": true,
    "constructor_visibility_public": true,
    "contains_tab": true,
    "definitions_top": true,
    "description_empty": true,
    "double_space": false,
    "empty_line_in_statement": {
      "allowChained": false
    },
    "empty_statement": true,
    "exit_or_check": true,
    "exporting": true,
    "functional_writing": {
      "ignoreExceptions": true
    },
    "global_class": true,
    "identical_form_names": true,
    "if_in_if": true,
    "indentation": {
      "ignoreExceptions": true,
      "alignTryCatch": false,
      "globalClassSkipFirst": false,
      "ignoreGlobalClassDefinition": false,
      "ignoreGlobalInterface": false
    },
    "inline_data_old_versions": true,
    "keyword_case": {
      "style": "upper",
      "ignoreExceptions": true,
      "ignoreGlobalClassBoundaries": false,
      "ignoreKeywords": [],
      "ignoreLowerClassImplmentationStatement": true,
      "ignoreGlobalClassDefinition": false,
      "ignoreGlobalInterface": false,
      "ignoreFunctionModuleName": false
    },
    "line_length": {
      "length": 120
    },
    "line_only_punc": {

      "ignoreExceptions": true
    },
    "local_class_naming": {
      "exception": "^LCX_.*$",
      "local": "^LCL_.*$",
      "test": "^LTCL_.*$"
    },
    "local_variable_names": {
      "expectedData": "^L._.*$",
      "expectedConstant": "^LC_.*$",
      "expectedFS": "^<L._.*>$"
    },
    "max_one_statement": true,
    "message_exists": true,
    "method_length": {
      "errorWhenEmpty": false,
      "statements": 100,
      "ignoreTestClasses": false,
      "exclude": ["zcl_aoc_check_69"]
    },
    "method_parameter_names": {
      "ignoreExceptions": true,
      "importing": "^I._.*$",
      "returning": "^R._.*$",
      "changing": "^C._.*$",
      "exporting": "^E._.*$",
      "ignoreNames": [
        "P_TASK"
      ]
    },
    "mix_returning": true,
    "msag_consistency": true,
    "nesting": {
      "depth": 6
    },
    "no_public_attributes": false,
    "object_naming": {
      "clas": "^ZC(L|X)\\_",
      "intf": "^ZIF\\_",
      "prog": "^Z",
      "fugr": "^Z",
      "tabl": "^Z",
      "ttyp": "^Z",
      "dtel": "^Z",
      "doma": "^Z",
      "msag": "^Z",
      "tran": "^Z",
      "enqu": "^Z",
      "auth": "^Z",
      "shlp": "^Z",
      "pinf": "^Z",
      "xslt": "^Z",
      "ssfo": "^Z",
      "ssst": "^Z",
      "idoc": "^Z"
    },
    "obsolete_statement": {
      "refresh": true,
      "compute": true,
      "add": true,
      "fieldSymbolStructure": true,
      "subtract": true,
      "multiply": true,
      "typePools": false,
      "move": true,
      "load": false,
      "withHeaderLine": true,
      "setExtended": true,
      "divide": true,
      "requested": true,
      "occurs": true
    },
    "parser_error": true,
    "preferred_compare_operator": {
      "badOperators": [
        "EQ",
        "><",
        "NE",
        "GE",
        "GT",
        "LT",
        "LE"
      ]
    },
    "remove_descriptions": {
      "ignoreExceptions": false,
      "ignoreWorkflow": false
    },
    "sequential_blank": {
      "lines": 4
    },
    "short_case": {
      "length": 1,
      "allow": ["column", "gv_ok_code"]
    },
    "space_before_colon": true,
    "start_at_tab": true,
    "superclass_final": true,
    "tabl_enhancement_category": true,
    "unreachable_code": true,
    "when_others_last": true,
    "whitespace_end": true
  }
}
